{
  "name": "keyword-pair-matcher",
  "version": "0.0.1",
  "description": "Highlight keyword pair",
  "keywords": [
    "highlight",
    "keyword",
    "pair",
    "matcher"
  ],
  "repository": "https://github.com/susisu/keyword-pair-matcher",
  "author": "Susisu <susisu2413@yahoo.co.jp>",
  "license": "MIT",
  "main": "lib/main.js",
  "scripts": {
    "lint": "eslint lib",
    "fix": "eslint lib --fix"
  },
  "engines": {
    "atom": ">=1.22.0 <2.0.0"
  },
  "activationCommands": {},
  "configSchema": {
    "keywordPairs": {
      "order": 1,
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": [],
      "description": "Pairs of start and end keywords (`start/end`)."
    },
    "highlightMatchingLineNumber": {
      "order": 2,
      "type": "boolean",
      "default": false,
      "description": "Highlight the line number of the matching keyword"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^5.6.0"
  }
}
